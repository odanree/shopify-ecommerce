{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd shopify-headless && npm run build",
  "devCommand": "cd shopify-headless && npm run dev",
  "installCommand": "cd shopify-headless && npm install",
  "framework": "nextjs",
  "outputDirectory": "shopify-headless/.next",
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" != \"main\" ]; then exit 1; else exit 0; fi",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
